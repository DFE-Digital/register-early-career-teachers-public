<%= govuk_summary_list do |sl|
  sl.with_row do |row|
    row.with_key(text: "Induction tutor")
    row.with_value(text: induction_tutor_name)
    row.with_action(text: "Change", href: "#", visually_hidden_text: "induction tutor name")
  end

  sl.with_row do |row|
    row.with_key(text: "Induction tutor email")
    row.with_value(text: induction_tutor_email)
    row.with_action(text: "Change", href: "#", visually_hidden_text: "induction tutor email")
  end

  sl.with_row do |row|
    row.with_key(text: "Local authority")
    row.with_value(text: local_authority_name)
  end

  sl.with_row do |row|
    row.with_key(text: "Address")
    row.with_value { address }
  end

  sl.with_row do |row|
    row.with_key(text: "Establishment status")
    row.with_value(text: status)
  end

  sl.with_row do |row|
    row.with_key(text: "Establishment type")
    row.with_value(text: establishment_type)
  end

  sl.with_row do |row|
    row.with_key(text: "Section 41 status")
    row.with_value(text: section_41_status)
  end

  sl.with_row do |row|
    row.with_key(text: "Administratrive district")
    row.with_value(text: administrative_district)
  end
end %>
