<%= tag.nav(class: navigation_classes, **navigation_attributes) do %>
  <ul class="x-govuk-secondary-navigation__list">
    <% items.each do |item| %>
      <li class="<%= item_classes(item) %>">
        <%= link_to item[:text], item[:href], link_attributes(item) %>
      </li>
    <% end %>
  </ul>
<% end %>
