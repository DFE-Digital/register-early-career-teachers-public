<%= govuk_back_link(href: ab_batch_actions_path) %> 

<%= turbo_stream_from "batch_progress_stream_#{@pending_induction_submission_batch.id}" %>

<div id="batch_progress_target_<%= @pending_induction_submission_batch.id %>" data-turbo="true">
  <%= 
    render partial: "appropriate_bodies/process_batch/actions/#{@pending_induction_submission_batch.batch_status}", 
    locals: { batch: @pending_induction_submission_batch } 
  %>
</div> 

<div class="govuk-!-margin-top-4">
  <%= govuk_details(summary_text: 'Detailed information') do %>
    <%= batch_progress_card(@pending_induction_submission_batch) %>
    <%= batch_raw_data_table(@pending_induction_submission_batch) %>
    <%= batch_processed_data_table(@pending_induction_submission_batch) %>
    <%= batch_download_data_table(@pending_induction_submission_batch) %>
    <%= batch_induction_periods_table(@pending_induction_submission_batch) %>
  <% end %>
</div>