<%
  page_data(
    title: 'DfE staff',
    error: false
  )
%>

<p class="govuk-body">
  <%= govuk_button_link_to 'Add new user', new_admin_user_path, secondary: true %>
<p>

<%=
  govuk_table(
    head: ["Name", "Roles"],
    rows: @users.map do |u|
      [
        govuk_link_to(u.name, admin_user_path(u), no_visited_state: true),
        role_name(u.role)
      ]
    end
  )
%>
